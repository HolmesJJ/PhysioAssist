<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#F9EE6E"></solid>
            <stroke
                android:color="#6D462C"
                android:width="15px"></stroke>
            <corners
                android:radius="15dp"></corners>
            <padding
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"
                android:bottom="10dp"></padding>
        </shape>
    </item>

    <item>
        <shape>
            <solid
                android:color="#F3ED79"></solid>
            <stroke
                android:color="#895B3A"
                android:width="15px"></stroke>
            <corners
                android:radius="15dp"></corners>
            <padding
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"
                android:bottom="10dp"></padding>
        </shape>
    </item>
</selector>